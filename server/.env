PORT=8080  # The port your server will listen on

# SMTP settings for email
SMTP_SERVER=smtp.example.com  # Your SMTP server address
SMTP_USER=your-email@example.com  # Your SMTP user/email
SMTP_PASS=your-email-password  # Your SMTP password

# SurrealDB connection settings
SURREALDB_URL=ws://localhost:8000  # The URL where SurrealDB is running
SURREALDB_USER=root  # SurrealDB username, usually 'root'
SURREALDB_PASSWORD=your-surreal-password  # Your SurrealDB password
SURREALDB_NS=your_namespace  # Namespace for SurrealDB
SURREALDB_DB=your_database  # Database name in SurrealDB

# MinIO settings
MINIO_URL=localhost:9000  # The URL where MinIO is running
MINIO_ACCESS_KEY_ID=minioadmin  # Your MinIO access key
MINIO_ACCESS_KEY_SECRET=minioadmin  # Your MinIO secret key
MINIO_BUCKET_NAME=your-bucket-name  # The name of the bucket you will use
